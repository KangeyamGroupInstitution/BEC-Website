          type="tel"
          id="parent_contact"
          name="parentContactNumber"
          placeholder="Enter your parent's contact number"
          value="<%= data.parentContactNumber %>"
          disabled
          required
          maxlength="10"
        />

        <label for="studentContactNumber"
