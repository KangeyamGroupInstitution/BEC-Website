<body>
    <form method="post" class="form1">
        <div class="p-text">Personal Details</div>
        <label for="name" id="name_l"
          >Full Name<span class="a">(*)</span></label
        >
        <input
          type="text"
          id="name"
          name="fullname"
          placeholder="<%= data.fullname %>"
          required
          disabled
        />

        <label for="father_name"
          >Parent/Guardian Name<span class="a">(*)</span></label
        >
        <input
          type="text"
          id="father_name"
          name="parentname"
          placeholder="<%= data.parentname %>"
          disabled
          required
        />

        <label for="dob">Date of Birth<span class="a">(*)</span></label>
        <input type="date" id="dob" value="<%= data.dateofbirth %>" name="dateofbirth" disabled required />

        <label for="mail_id">Mail id<span class="a">(*)</span></label>
        <input
          type="email"
          id="mail_id"
          name="email"
          value=""
          placeholder="<%= data.email %>"
          disabled
          required
        />
        <label for="aadhar">Aadhar Number</label>
        <input
          type="number"
          id="Aadhar"
          name="aadharnumber"
          value="<%= data.aadharnumber %>"
          disabled
          placeholder="Enter your Aadhar number"
        />

        <div class="sel_address" id="invoice"></div>
        <label for="country" id="country">
          Select Country<span class="a">(*)</span>
        </label>
        <select name="state" id="countySel" size="1" disabled required>
          <option value="" selected="selected"><%= data.state %></option>
        </select>

        <label for="State" id="State"
          >Select State<span class="a">(*)</span></label
        >
        <select name="country" id="stateSel" size="1" disabled required>
          <option value="" selected="selected">
            <%= data.country %>
          </option>
        </select>

        <label for="district" id="district"
          >Select District<span class="a">(*)</span></label
        >
        <select name="district" id="districtSel" size="1" disabled required>
          <option value="<%= data.district %>" selected="selected">
            <%= data.district %>
          </option>
        </select>
        <label for="address">Address<span class="a">(*)</span></label>
        <input
          type="text"
          id="address"
          name="address"
          placeholder="Enter your address"
          value="<%= data.address %>"
          disabled
          required
        />

        <label for="pincode">Pincode<span class="a">(*)</span></label>
        <input
          type="text"
          id="pincode"
          name="pincode"
          placeholder="Enter your pincode"
          value="<%= data.pincode %>"
          disabled
          required
        />

        <label for="parent_contact"
          >Parent's Contact Number<span class="a">(*)</span></label
        >
